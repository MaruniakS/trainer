<% exercises.each do |e|%>
    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 exercise-list-item">
      <%= link_to e, class: 'exercise-list-link text-center', 'data-toggle'=> 'modal' do%>
          <div class="exercise-list-hover">
            <div class="exercise-list-hover-content">
              <i class="fa fa-eye fa-3x"></i>
            </div>
          </div>
          <%= image_tag(e.exercise_image, class: 'img-responsive') %>
      <% end %>
      <div class="exercise-list-caption">
        <h4><%= e.name %></h4>
      </div>
    </div>
<% end %>